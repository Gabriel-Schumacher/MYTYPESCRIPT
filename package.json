{
  "name": "mytypescript",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watchTailWind": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "buildTailWind": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
    "serveVite": "vite --port 3000",
    "dev": "npm-run-all --parallel --race watchTailWind serveVite",
    "build": "npx tsc && vite build && npm run buildTailWind"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.1.1"
  },
  "dependencies": {
    "daisyui": "^2.50.0"
  }
}
